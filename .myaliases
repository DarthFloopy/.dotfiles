# .myaliases - my custom shell aliases (shared between shells)

alias -- -='cd -'
alias ...=../..
alias ....=../../..
alias .....=../../../..
alias ......=../../../../..
alias 1='cd -'
alias 2='cd -2'
alias 3='cd -3'
alias 4='cd -4'
alias 5='cd -5'
alias 6='cd -6'
alias 7='cd -7'
alias 8='cd -8'
alias 9='cd -9'
alias _='sudo '

alias cp='cp -v'

alias ls='ls -F --color=auto'
alias l='ls -lh'
alias la='ls -A'
alias ll='ls -lAh'
alias l.='ls -d .*'
alias ll.='ls -lhd .*'

alias sl='ls'

alias gs='git status'
alias gA='git add -A'

alias info='info --vi-keys'


